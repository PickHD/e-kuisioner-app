{
  "name": "e-kuisioner-app",
  "version": "1.0.4",
  "description": "A SSR Web App Quisioner for User & Admin. Created with Framework Express v4.17.1 , Databases with Sequelize ORM v6.3.5 (dialect postgres). Rendered with EJS Template Engine.",
  "main": "app/server.js",
  "scripts": {
    "start": "node app/server.js",
    "dev": " DEBUG=express-session nodemon app/server.js",
    "heroku-postbuild": "webpack --config app/config/webpack.config.js",
    "build": "webpack --config app/config/webpack.config.js",
    "lint": "eslint --fix app",
    "ejsLint": "ejslint app/views/**/*.ejs",
    "migrate": "cd ./app && npx sequelize-cli db:migrate all",
    "seed": "cd ./app && npx sequelize-cli db:seed:all",
    "undo_migrate": "cd ./app && npx sequelize-cli db:migrate:undo:all",
    "test": "mocha './test/**/*.test.js' --exit || true --timeout 10000",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test"
  },
  "author": "PickHD <taufikjanuar35@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-csp-header": "^4.0.0",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.4.13",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.11.0",
    "file-loader": "^6.2.0",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0",
    "style-loader": "^2.0.0",
    "supertest": "^5.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  }
}
